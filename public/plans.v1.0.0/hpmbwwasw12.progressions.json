{
  "name": "Highlander Power Marathon Build with Wheels and Some Water 12",
  "short_code": "hpmbwwasw12",
  "description": "12-week hybrid plan combining marathon preparation, barbell strength, bike, and optional swim technique for experienced to very experienced athletes.",
  "duration_weeks": 12,
  "current_week": 1,
  "status": "active",
  "plan_type": "marathon_power",
  "config": {
    "experience": "experienced|very_experienced",
    "days_per_week": 6,
    "optional_day": 1,
    "long_run_day": "Saturday",
    "rest_day": "Sunday"
  },
  "constraints": {
    "defaults": {
      "long_run_day": "Saturday",
      "mandatory_ride_day": "Friday",
      "optional_long_ride_day": "Sunday",
      "rest_day": "Sunday",
      "heavy_lower_days": ["Tuesday", "Thursday"]
    },
    "allowed_modifications": {
      "long_run_day": ["Monday","Tuesday","Wednesday","Thursday","Friday","Saturday","Sunday"],
      "mandatory_ride_day": ["Wednesday","Friday"],
      "optional_long_ride_day": ["Sunday"],
      "rest_day": ["Monday","Friday","Sunday"]
    },
    "validators": [
      { "name": "no_strength_on_long_run_day", "error": "Long run cannot be scheduled on a strength day.", "rule": "DAY(long_run_day) != ANY(heavy_lower_days)" },
      { "name": "buffer_from_heavy_lower_before_after_long_run", "error": "Long run must have at least 1 day buffer from heavy lower lifts.", "rule": "ABS(DIFF_DAYS(long_run_day, Tuesday)) >= 1 && ABS(DIFF_DAYS(long_run_day, Thursday)) >= 1" },
      { "name": "no_long_run_and_long_ride_same_day", "error": "Long run cannot share a day with the optional long ride.", "rule": "long_run_day != optional_long_ride_day" },
      { "name": "mandatory_ride_not_on_long_run_or_strength", "error": "Mandatory ride cannot land on long run day or a heavy strength day.", "rule": "mandatory_ride_day != long_run_day && mandatory_ride_day != Tuesday && mandatory_ride_day != Thursday" },
      { "name": "rest_not_on_long_run_day", "error": "Rest day cannot be the same as the long run day.", "rule": "rest_day != long_run_day" },
      { "name": "avoid_back_to_back_long_endurance", "error": "Avoid stacking long run and long ride on consecutive days unless 'very_experienced' is selected.", "rule": "(experience == 'very_experienced') || ABS(DIFF_DAYS(long_run_day, optional_long_ride_day)) >= 2" }
    ],
    "auto_adjustments": [
      {
        "name": "tempo_swap_when_ride_moves_to_wed",
        "when": "mandatory_ride_day == 'Wednesday'",
        "do": [
          "MOVE_SESSION(type='bike', focus='intervals_or_endurance') TO 'Wednesday'",
          "MOVE_SESSION(type='run', focus='tempo') TO 'Friday'"
        ],
        "note": "Keeps midweek quality balance: Mon run intervals, Wed bike quality, Fri tempo."
      },
      {
        "name": "disable_optional_long_ride_if_conflict",
        "when": "optional_long_ride_day == long_run_day",
        "do": ["REMOVE_SESSION(type='bike', focus='long', day=optional_long_ride_day)"],
        "note": "If athlete places the long run on Sunday, the optional long ride is removed for that week."
      },
      {
        "name": "shift_rest_day_if_conflict",
        "when": "rest_day == long_run_day",
        "do": ["IF 'Monday' != long_run_day && 'Monday' NOT IN heavy_lower_days THEN SET rest_day='Monday' ELSE SET rest_day='Friday'"],
        "note": "Rest moves off the long run day, preferring Monday unless it collides with heavy strength rules."
      }
    ],
    "priority": ["long_run_day","heavy_lower_days","mandatory_ride_day","rest_day","optional_long_ride_day"]
  },

